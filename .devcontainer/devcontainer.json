{
  "name": "First EJB Sample",
  "image": "mcr.microsoft.com/devcontainers/java:1-8-bullseye",
  "features": {
    "ghcr.io/devcontainers/features/maven:1": {
      "version": "3.9"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "redhat.java"
      ]
    }
  },
  "postCreateCommand": "mvn -q -DskipTests package"
}
